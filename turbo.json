{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "lint", "format:check"],
      "outputs": ["dist/**", "packages/cli/dist/**"]
    },
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}
