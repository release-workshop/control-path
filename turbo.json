{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "lint", "typecheck", "format:check"],
      "outputs": ["dist/**", "packages/cli/dist/**"]
    },
    "test": {
      "dependsOn": ["^build", "build"],
      "outputs": ["coverage/**"]
    }
  }
}
